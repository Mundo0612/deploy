<br/>

<table class="table table-striped table-dark">
  <thead class="thead-dark">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Title</th>
      <th scope="col">ISBN</th>
      <th scope="col">Book ID</th>
    </tr>
  </thead>

    <tbody>
   <% @bo.each do |b|  %>
    <tr>
      <th scope="row"> <%= b.id %> </th>
      <td> <%= b.title %> </td>
      <td> <%= b.isbn %> </td>
      <td> <%= b.book_id %> </td>
    </tr>
        <% end %>
     </tbody>
</table>